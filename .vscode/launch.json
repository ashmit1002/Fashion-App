{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Debug FastAPI App",
        "type": "python",
        "request": "launch",
        "module": "uvicorn",
        "args": [
          "app.main:app",       // use app.main if your FastAPI instance is inside app/main.py
          "--reload"
        ],
        "jinja": true,
        "justMyCode": false,
        "console": "integratedTerminal"
      }
    ]
  }
  